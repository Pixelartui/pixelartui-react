{
  "name": "pixelartui",
  "version": "0.0.1",
  "description": "Pixel Art Component library",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll --verbose",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "lint": "eslint ./src/ --ext .ts,.tsx",
    "lint:fix": "eslint ./src --ext .ts,.tsx --fix",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muyaszed/pixelartui.git"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "keywords": [
    "component",
    "library"
  ],
  "author": "Yazed Jamal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muyaszed/pixelartui/issues"
  },
  "homepage": "https://github.com/muyaszed/pixelartui#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@chromatic-com/storybook": "^1.9.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.30.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.2.10",
    "@storybook/addon-interactions": "^8.2.10",
    "@storybook/addon-links": "^8.2.10",
    "@storybook/addon-onboarding": "^8.2.10",
    "@storybook/blocks": "^8.2.10",
    "@storybook/react": "^8.2.10",
    "@storybook/react-vite": "^8.2.10",
    "@storybook/test": "^8.2.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "standard-version": "^9.5.0",
    "storybook": "^8.2.10",
    "styled-components": "^6.1.19",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  }
}
